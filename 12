#include <stdio.h>
#include <stdlib.h>
#include <string.h>   
#define MAX 100
struct Employee {
    int  key;          
    char name[30];
    int  isOccupied;   
};
int hashFunction(int key, int m) {
    return key % m;
}
int main() {
    struct Employee HT[MAX];
    int m, n, i;
    int key, index;
    char name[30];
    printf("Enter hash table size (m): ");
    scanf("%d", &m);
    if (m <= 0 || m > MAX) {
        printf("Invalid table size\n");
        return 0;
    }
    for (i = 0; i < m; i++) {
        HT[i].isOccupied = 0;
    }
    printf("Enter number of employee records (N): ");
    scanf("%d", &n);
    for (i = 0; i < n; i++) {
        printf("\nEnter Employee Key (4-digit): ");
        scanf("%d", &key);
        printf("Enter Employee Name: ");
        scanf("%s", name);
        index = hashFunction(key, m);
        while (HT[index].isOccupied == 1) {
            index = (index + 1) % m;
        }
        HT[index].key = key;
        strcpy(HT[index].name, name);  
        HT[index].isOccupied = 1;
        printf("Record inserted at address: %d\n", index);
    }
    printf("\nHash Table Contents:\n");
    printf("Address\tKey\tName\n");
    for (i = 0; i < m; i++) {
        if (HT[i].isOccupied == 1)
            printf("%d\t%d\t%s\n", i, HT[i].key, HT[i].name);
        else
            printf("%d\t--\t--\n", i);
    }
    return 0;
}
